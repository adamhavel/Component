{
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "brace-style": 2,
        "indent": [2, 4, {"SwitchCase": 1}],
        "eqeqeq": [2, "smart"],
        "no-console": 1
    }
}
